mixin sponsor-logo(url, delay)
  .col-md-3.col-sm-4.col-4.col-lg-3.mb-5.mb-lg-0(
    data-aos='fade' 
    data-aos-delay=delay?delay:'400'
  )
    img.img-fluid.custom-img(src="static/images/"+url alt='Image')

